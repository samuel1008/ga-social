<h1> all convo </h1>
<% for conversation in @conversations %>
  <% conversation.participants.each do |participant| %>
    <div>
      <%= participant.name %>
    </div>
    <div>
      <%= link_to conversation.subject, conversation_path(conversation) %>
    </div>
  <% end %>
<% end %>
<%= link_to "new conversations", new_conversation_path %>
